{
    "name": "arubacao/tld-checker",
    "description": "place holder description",
    "keywords": [
        "tld-checker"
    ],
    "homepage": "https://github.com/arubacao/tld-checker",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Lass",
            "email": "arubacao@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-intl": "*"
    },
    "require-dev": {
        "arubacao/php-cs-fixer-config": "^0.1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~6.0"
    },
    "suggest": {
        "illuminate/validation": "Required for Laravel validation"
    },
    "autoload": {
        "psr-4": {
            "Arubacao\\TldChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arubacao\\TldChecker\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "vendor/bin/php-cs-fixer fix ./ --config=vendor/arubacao/php-cs-fixer-config/laravel.php_cs -vv"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arubacao\\TldChecker\\TldCheckerServiceProvider"
            ]
        }
    }
}
